<!DOCTYPE html>
<html>
    <head>
        <title><%= full_title(yield(:title)) %></title>
        <%= render 'layouts/partials/rails_default' %>
        <%= render 'layouts/partials/shim' %>
    </head>
    <body>
      <%= render 'layouts/partials/header' %>
      <div class="container">
          <%= yield %>
          <%= render 'layouts/partials/footer' %>
          <%= debug(params) if Rails.env.development? %>
      </div>
    </body>
</html>
